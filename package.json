{
  "name": "timba-stack",
  "description": "Timba Stack is a Remix based template for building fullstack web applications.",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "tsx watch --tsconfig ./tsconfig.node.json --ignore \"*.timestamp*.mjs\" ./src/server/server.ts | pino-pretty",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production node ./build/server/server.js",
    "typecheck": "tsc --project tsconfig.app.json && tsc --project tsconfig.node.json",
    "format": "prettier --write . \"src/**/*.{ts,tsx,json,mjs,yml}\" --cache",
    "format:check": "prettier --check . \"src/**/*.{ts,tsx,json,mjs,yml}\" --cache"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.0",
    "@remix-run/express": "^2.12.1",
    "@remix-run/node": "^2.12.1",
    "@remix-run/react": "^2.12.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.34.1",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "http-errors": "^2.0.0",
    "isbot": "^4.1.0",
    "lucide-react": "^0.451.0",
    "pg": "^8.13.0",
    "pino": "^9.4.0",
    "pino-http": "^10.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.5.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@remix-run/dev": "^2.12.1",
    "@total-typescript/tsconfig": "^1.0.4",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/eslint": "^9.6.1",
    "@types/express": "^4.17.21",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.25.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc-1460d67c-20241003",
    "globals": "^15.10.0",
    "pino-pretty": "^11.2.2",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.13",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.1",
    "vite": "^5.4.8",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
