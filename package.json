{
  "name": "timba-stack",
  "description": "Timba Stack is a Remix based template for building fullstack web applications.",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "node ./server.js",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "tsc",
    "format": "prettier --write . \"app/**/*.{ts,tsx,json,mjs,yml}\" --cache",
    "format:check": "prettier --check . \"app/**/*.{ts,tsx,json,mjs,yml}\" --cache"
  },
  "dependencies": {
    "@remix-run/express": "^2.12.1",
    "@remix-run/node": "^2.12.1",
    "@remix-run/react": "^2.12.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.19.2",
    "isbot": "^4.1.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@remix-run/dev": "^2.12.1",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/compression": "^1.7.5",
    "@types/eslint": "^9.6.1",
    "@types/express": "^4.17.20",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc-1460d67c-20241003",
    "globals": "^15.10.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.1",
    "vite": "^5.4.8",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
